{
  "manifest_version": 3,
  "name": "RM ↔ OC Currency Converter",
  "version": "1.0.0",
  "description": "Quick currency converter between RM and OC currencies",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "RM ↔ OC Converter"
  },
  "permissions": [
    "storage"
  ]
}
